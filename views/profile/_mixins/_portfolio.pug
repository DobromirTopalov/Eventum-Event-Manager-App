        // end /.col-md-4
        
    .row
        .col-md-12
            .product-title-area
                .product__title
                    h2 Newest Products
                a.btn.btn--sm(href='#') See all Items
            // end /.product-title-area
        // end /.col-md-12
        // start .col-md-4
        .col-md-6.col-sm-6
            // start .single-product
            .product.product--card
                .product__thumbnail
                    img(src='images/p4.jpg', alt='Product Image')
                    .prod_btn
                        a.transparent.btn--sm.btn--round(href='single-product.html') More Info
                        a.transparent.btn--sm.btn--round(href='single-product.html') Live Demo
                    // end /.prod_btn
                // end /.product__thumbnail
                .product-desc
                    a.product_title(href='#')
                        h4 Yannan Na nakka muka
                    ul.titlebtm
                        li
                            img.auth-img(src='images/auth3.jpg', alt='author image')
                            p
                                a(href='#') AazzTech
                        li.product_cat
                            a(href='#')
                                img(src='images/cathtm.png', alt='category image')
                                | Plugin
