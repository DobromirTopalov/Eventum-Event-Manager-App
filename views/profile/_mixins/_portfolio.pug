mixin user-portfolio()
    .row
        .col-md-4.col-sm-4
            .author-info.mcolorbg4
                p Total Items
                h3 4,369
        // end /.col-md-4
        .col-md-4.col-sm-4
            .author-info.pcolorbg
                p Total sales
                h3 36,957
        // end /.col-md-4
        .col-md-4.col-sm-4
            .author-info.scolorbg
